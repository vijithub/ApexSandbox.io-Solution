public Boolean isLeapYear(Integer year) {
    //code here
    // year divisible by 4 is a leap year, 
    // if it is not a century
    Boolean bLeapYear = false;

    if((Math.mod(year,4) == 0)){
        bLeapYear = true;
    } 
    if((Math.mod(year,100) != 0) || (Math.mod(year,400) != 0)){
        bLeapYear = false;
    } 

    return bLeapYear;

}
